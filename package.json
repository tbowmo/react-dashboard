    "@types/mocha": "10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chai": "^4.3.7",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "8.2.0",
    "dotenv-load": "^2.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-newlines": "^1.3.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "8.1.0",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "lerna": "^7.1.1",
    "lint-staged": "^13.1.2",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "2.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install": "lerna run install",
    "start": "dotenv-load lerna run start",
    "build": "dotenv-load lerna run build",
    "format": "prettier --write packages/**/*.ts{,x}",
    "lint": "eslint packages/*/src/**/*.ts{,x} packages/*/*.ts{,x}",
    "postinstall": "husky install",
    "coverage": "nyc --clean --cache false mocha --forbid-only -r ts-node/register -r source-map-support/register 'packages/*/src/**/*.test.ts{,x}'",
    "postcoverage": "nyc check-coverage --functions 50 --branches 50 --statements 90"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings 0 packages/*/src/**/*.ts{,x}",
    "*.{yml,yaml,json}": "prettier -c"
  },
  "packageManager": "yarn@3.6.1"
}
