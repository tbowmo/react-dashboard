{
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "declaration": true,
    "jsx": "react",
    "lib": ["dom", "es5", "es2017", "esnext"],
    "module": "ES2022",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmit": true,
    "noEmitOnError": false,
    "noErrorTruncation": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./lib",
    "pretty": true,
    "removeComments": false,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2019",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "compileOnSave": false
}
