{
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "declaration": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es2017",
      "esnext"
    ],
    "locale": "en",
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmit": true,
    "noEmitOnError": false,
    "noErrorTruncation": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "./lib",
    "pretty": true,
    "removeComments": false,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2019"
  },
  "compileOnSave": false
}
